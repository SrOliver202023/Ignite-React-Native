{
  "compilerOptions": {
    "types": [
      "node",
      "express",
      "react-native"
    ],
    "target": "es2016",
    "jsx": "preserve",
    "experimentalDecorators": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}